@{
    ViewBag.Title = "Register";
    Layout = "~/Views/Shared/Admin_Layout.cshtml";
}
<div style="background-color: yellow;  padding-left: 370px; padding-top: 4px; padding-bottom: 2px">
    <h4 style="font-size: 35px"> Give your date here for sign up for FREE! </h4>
</div>
<br />
<form action="/User/Registration" method="post" enctype="multipart/form-data">
    <div style="padding-right: 400px ; padding-left: 400px">



        <div class="form-group">
            <label for="name">First Name</label>
            <input type="text" class="form-control" id="FName" name="FName" required="required">
        </div>
        <div class="form-group">
            <label for="name">Last Name</label>
            <input type="text" class="form-control" id="LName" name="LName" required="required">
        </div>

        <div class="form-group">
            <label for="director">Email Address</label>
            <input type="text" class="form-control" id="email" name="email" required="required">
        </div>

        <div class="form-group">
            <label for="type">New Password</label>
            <input type="password" class="form-control" id="pass" name="pass" required="required">
        </div>
        <div class="form-group">
            <label for="type">Type Password Again</label>
            <input type="password" class="form-control" id="pass1" name="pass1" required="required">
        </div>
        <div class="form-group">
            <label for="time">Phone Number </label>
            <input type="text" class="form-control" id="cell" name="cell" required="required">
        </div>

      

        <label for="genre">Gender</label>
        <select class="form-control" id="gender" name="gender" @*style="width:300px;height:auto"*@>
            <option value="">--Select--</option>
            <option value="Male">Male</option>
            <option value="Female">Female</option>

        </select>
        
        <br/><br /><br />
        
        <a href="http://localhost:13234/Admin"><button type="submit" class="btn btn-primary">Register</button></a> 
    </div>
</form>
<br /><br /><br />


